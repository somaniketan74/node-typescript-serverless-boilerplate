    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch via NPM",
            "runtimeExecutable": "npm",
            "cwd": "${workspaceRoot}",
            "runtimeVersion": "12.16.0",
            "runtimeArgs": [
                "run",
                "debug"
            ],
            "preLaunchTask": "tsc: build - tsconfig.json",
            "port": 9229,
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
    ]
}